<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph.xml"
    app:startDestination="@id/blankFragment">

    <fragment
        android:id="@+id/blankFragment"
        android:name="com.claucas90.e2m6.VIEW.BlankFragment"
        android:label="BlankFragment" >
        <action
            android:id="@+id/action_blankFragment_to_listaFragment"
            app:destination="@id/listaFragment" />
    </fragment>
    <fragment
        android:id="@+id/listaFragment"
        android:name="com.claucas90.e2m6.VIEW.ListaFragment"
        android:label="ListaFragment" />
    <activity
        android:id="@+id/mainActivity"
        android:name="com.claucas90.e2m6.VIEW.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>